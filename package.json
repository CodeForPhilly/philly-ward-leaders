{
  "name": "philly-ward-leaders",
  "description": "Shining a spotlight on the powerful players behind Philadelphia elections.",
  "version": "1.0.0",
  "author": "timwis <tim@timwis.com>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot",
    "build": "vite build",
    "test": "jest --testPathIgnorePatterns ./e2e",
    "lint": "standard --plugin html 'src/**/*.{js,vue}'",
    "e2e": "playwright test",
    "e2e-ci": "playwright install --with-deps && playwright test",
    "serve-http": "http-server ./public -p 8080",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mapbox/leaflet-pip": "^1.1.0",
    "@vue-leaflet/vue-leaflet": "0.10.1",
    "@vue/compat": "^3.3.9",
    "axios": "^1.6.2",
    "bulma": "^0.9.4",
    "contentful": "^10.6.14",
    "leaflet": "^1.9.4",
    "leaflet-geocoder-mapzen": "^1.9.4",
    "marked": "^11.0.0",
    "nanoid": "^5.0.4",
    "ordinal": "^1.0.3",
    "query-string": "^8.1.0",
    "snapsvg-cjs": "^0.0.6",
    "vue": "^3.3.9",
    "vue-bulma-tooltip": "^2.0.0-alpha.0",
    "vue-directive-tooltip": "^1.4.0",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@playwright/test": "^1.40.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-minify-webpack-plugin": "^0.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint-plugin-html": "^7.1.0",
    "file-loader": "^6.2.0",
    "http-server": "^14.1.1",
    "imports-loader": "^4.0.1",
    "jest": "^29.7.0",
    "jest-serializer-vue": "^3.1.0",
    "node-sass": "^9.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "3.1.0",
    "sass": "^1.70.0",
    "sass-loader": "^13.3.2",
    "standard": "^17.1.0",
    "vite": "^5.0.8",
    "vue-loader": "^17.3.1",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/app.spec.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>node_modules/jest-vue",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "jest",
      "test",
      "beforeEach",
      "FormData"
    ]
  }
}
